{
  "name": "we-ui",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start clear": " expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "打包生产apk": "cd android && ./gradlew assembleRelease",
    "打包ios模拟器": "eas build --platform ios --profile ios-simulator",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build android preview": "eas build --platform android --profile preview",
    "build android": "eas build --platform android",
    "打包网页": "expo export --platform web",
    "test": "jest --watch --coverage=true",
    "重装依赖": "rm -rf node_modules package-lock.json yarn.lock && yarn install",
    "生成安卓代码": "npx expo prebuild --platform android --clean",
    "lint": "expo lint",
    "生成演示源码": "node ./scripts/generate-demo-source.js"
  },
  "dependencies": {
    "@expo/vector-icons": "14.0.4",
    "@react-navigation/native": "^7.0.14",
    "expo": "^52.0.28",
    "expo-constants": "~17.0.4",
    "expo-dev-client": "~5.0.9",
    "expo-font": "~13.0.3",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.16",
    "expo-splash-screen": "~0.29.20",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.7",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "~0.77.0",
    "react-native-gesture-handler": "~2.22.1",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "~5.1.0",
    "react-native-screens": "~4.5.0",
    "react-native-svg": "15.11.1",
    "react-native-syntax-highlighter": "^2.1.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "~13.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.25.9",
    "@babel/runtime": "7.25.9",
    "@types/jest": "29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "18.0.7",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-universe": "13.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "jest-expo": "~52.0.3",
    "prettier": "3.3.2",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native@~0.77.0",
        "react-native-reanimated@~3.16.7",
        "react-native-gesture-handler@~2.22.1",
        "react-native-screens@~4.5.0",
        "react-native-safe-area-context@~5.1.0",
        "react-native-webview@~13.12.5"
      ]
    }
  },
  "private": true
}
